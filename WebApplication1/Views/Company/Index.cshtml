@model IEnumerable<WebApplication1.Models.Company>

<h1>Companies</h1>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Industry</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var company in Model)
        {
            <tr>
                <td>@company.Id</td>
                <td>@company.Name</td>
                <td>@company.Industry</td>
            </tr>
        }
    </tbody>
</table>